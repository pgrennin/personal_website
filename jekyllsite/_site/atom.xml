    &lt;tr&gt;
      &lt;td&gt;Chess&lt;/td&gt;
      &lt;td&gt;&lt;svg width=&quot;24px&quot; height=&quot;24px&quot; stroke-width=&quot;1.5&quot; viewBox=&quot;0 0 24 24&quot; fill=&quot;none&quot; xmlns=&quot;http://www.w3.org/2000/svg&quot; color=&quot;#000000&quot;&gt;&lt;path d=&quot;M12 22c5.523 0 10-4.477 10-10S17.523 2 12 2 2 6.477 2 12s4.477 10 10 10z&quot; stroke=&quot;#000000&quot; stroke-width=&quot;1.5&quot; stroke-linecap=&quot;round&quot; stroke-linejoin=&quot;round&quot;&gt;&lt;/path&gt;&lt;path d=&quot;m 10.9571,8.8402603 c 0.11431,2.7038257 2.798255,3.0645557 4.480148,4.7170917 0.726816,0.728975 1.188734,1.129834 1.410111,1.702892 0.221376,0.573061 1.462314,-0.442363 1.704629,-0.782801 0.242316,-0.340437 0.459965,-0.817816 0.411351,-1.207708 -0.04861,-0.389893 -3.692825,-6.4115628 -3.692825,-6.4115628 l 0.346797,-1.870523 c 0,0 -0.576444,0.093028 -1.185611,0.3262455 -0.669564,0.2563406 -1.30528,0.6306111 -1.30528,0.6306111 -3.8837588,-0.7806282 -8.0987515,1.6115714 -8.0987515,5.8443532 0,4.872342 3.8557378,7.210331 6.7146625,7.223481 2.69079,0 3.905406,-0.842829 4.683296,-1.734187&quot; stroke=&quot;#000000&quot; stroke-width=&quot;1.5&quot; stroke-linecap=&quot;round&quot; stroke-linejoin=&quot;round&quot;&gt;&lt;/path&gt;&lt;/svg&gt;
